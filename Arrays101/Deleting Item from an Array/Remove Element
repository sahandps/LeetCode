from typing import List

def removeElement( nums: List[int], val: int) -> int:
    k = 0
    i = 0
    while i < len(nums):
        if nums[i] != val:
            nums[k] = nums[i]
            i = i +1
            k = k + 1
        i = i + 1    
    return k        



nums = [0,1,2,2,3,0,4,2]
val = 2
removeElement(nums, val)